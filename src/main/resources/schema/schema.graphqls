type Query {
    getModel(id: ID!): Model3D
}

type Mutation {
    generateModel(prompt: String!): Model3D!
}

type Model3D {
    id: ID!
    name: String!
    description: String
    complexity: Int
    vertices: Int
    polygons: Int
    status: Status!
    createdAt: String!
    modelUrl: String
}

enum Status {
    EN_COURS
    COMPLETED
    FAILED
}